                <value id="ppcheckout" type="host">checkout.paypal.com</value>
                <value id="cpaypal" type="host">c.paypal.com</value>
                <value id="fpdbs" type="host">fpdbs.paypal.com</value>
                <value id="fpdbs_sandbox" type="host">fpdbs.sandbox.paypal.com</value>
