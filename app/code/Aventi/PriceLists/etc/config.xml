<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <price_lists>
            <general>
                <enable>1</enable>
                <categories_logged_in>1</categories_logged_in>
                <restrict_product_lists>0</restrict_product_lists>
                <add_to_cart_layout_name>product.info.addtocart</add_to_cart_layout_name>
                <disable_tier_pricing>1</disable_tier_pricing>
                <change_template>0</change_template>
                <add_to_cart_layout_name>0</add_to_cart_layout_name>
                <type_price>min</type_price>
            </general>
        </price_lists>
    </default>
</config>
