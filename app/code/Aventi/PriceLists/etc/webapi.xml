<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/aventi-pricelists/pricelist">
		<service class="Aventi\PriceLists\Api\PriceListRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Aventi_PriceLists::PriceList_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/aventi-pricelists/pricelist/search">
		<service class="Aventi\PriceLists\Api\PriceListRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Aventi_PriceLists::PriceList_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/aventi-pricelists/pricelist/:pricelistId">
		<service class="Aventi\PriceLists\Api\PriceListRepositoryInterface" method="get"/>
		<resources>
			<resource ref="Aventi_PriceLists::PriceList_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/aventi-pricelists/pricelist/:pricelistId">
		<service class="Aventi\PriceLists\Api\PriceListRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Aventi_PriceLists::PriceList_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/aventi-pricelists/pricelist/:pricelistId">
		<service class="Aventi\PriceLists\Api\PriceListRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Aventi_PriceLists::PriceList_delete"/>
		</resources>
	</route>
