		<service class="Aventi\PriceLists\Api\PriceListProductsRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Aventi_PriceLists::PriceListProducts_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/aventi-pricelists/pricelistproducts/search">
