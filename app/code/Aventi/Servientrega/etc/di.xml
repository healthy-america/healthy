<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="Guide" xsi:type="object">Aventi\Servientrega\Console\Command\Guide</item>
                <item name="Shipping" xsi:type="object">Aventi\Servientrega\Console\Command\GenerateShipping</item>
                <item name="Review" xsi:type="object">Aventi\Servientrega\Console\Command\ReviewShipment</item>
            </argument>
        </arguments>
    </type>
    <preference for="Magento\Shipping\Block\Tracking\Popup" type="Aventi\Servientrega\Block\Tracking\Popup"/>
    <preference for="Magento\InventorySourceDeductionApi\Model\SourceDeductionService" type="Aventi\Servientrega\Model\SourceDeductionService"/>
</config>
