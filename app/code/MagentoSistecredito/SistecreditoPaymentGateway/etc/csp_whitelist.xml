<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="sistecredito-script.com" type="host">*.blob.core.windows.net/*</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="sistecredito-static.com" type="host">*.sistecredito.com/*</value>
                <value id="sistecredito-static.net" type="host">*.blob.core.windows.net/*</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="sistecredito.com-frame" type="host">*.blob.core.windows.net/*</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
